  </head>
  <body>

    <nav class="navbar navbar-main navbar-fixed-top">
    <div class="container">
      <!-- Brand and toggle get grouped for better mobile display -->
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>

        {% if show_brand %}
          <a class="navbar-brand" href="/">{{ brand }}</a>
        {% endif %}
      </div>

      <!-- Collect the nav links, forms, and other content for toggling -->
      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <ul class="nav navbar-nav">
          {% for item in left_menu %}
            <li class="{{ item.active ? 'active' : '' }}"><a href="{{ item.href }}">{{ item.text }} <span class="sr-only">(current)</span></a></li>
          {% endfor %}
        </ul>

        <ul class="nav navbar-nav navbar-right">
          {% for item in right_menu %}
            <li class="{{ item.active ? 'active' : '' }}"><a href="{{ item.href }}">{{ item.text }} <span class="sr-only">(current)</span></a></li>
          {% endfor %}


          {% if user %}
            <li class="dropdown">
              <a href="#" class="dropdown-toggle" data-toggle="dropdown">{{ user.name ? user.name : user.username }} <span class="caret"></span></a>
              <ul class="dropdown-menu">
                <li><a href="/user">Account</a></li>
                <li><a href="/api/v1/forms/auth/logout">Logout</a></li>
              </ul>
            </li>
          {% endif %}


        </ul>
      </div>
    </div>
  </nav>

    {% block body %}

    {% endblock %}

  </body>

  <script src="/javascripts/init.js"></script>
  <script src="/javascripts/audio.js"></script>
  <script src="/javascripts/parseParams.js"></script>
  <script src="/javascripts/pswd.js"></script>
  <script src="/javascripts/vue.js"></script>
  <script src="/javascripts/script.js"></script>

</html>
